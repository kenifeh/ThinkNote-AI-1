# Clerk Authentication (Required)
# Get these from https://clerk.com/
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_publishable_key_here
CLERK_SECRET_KEY=sk_test_your_secret_key_here

# Database (Required)
# Get this from your PostgreSQL provider (e.g., Supabase, Neon, Railway)
DATABASE_URL="postgresql://username:password@localhost:5432/thinknote_ai"

# OpenAI API (Required for AI features)
# Get this from https://platform.openai.com/
OPENAI_API_KEY=your_openai_api_key_here

# DigitalOcean Spaces (Required for file storage)
# Get these from https://cloud.digitalocean.com/spaces
DO_SPACES_ENDPOINT=https://nyc3.digitaloceanspaces.com
DO_SPACES_KEY=your_spaces_access_key_here
DO_SPACES_SECRET=your_spaces_secret_key_here
DO_SPACES_BUCKET=your-spaces-bucket-name
DO_SPACES_REGION=nyc3

# Next.js
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here
